<style>
    /*******************************
       FONT TYPE STYLES
    *******************************/
    @charset "utf-8";

    /*- BLADE UI FONT SET -*/
    @font-face {
        font-family: 'vag_rounded_bold';
        src: url('/fonts/vag_rounded_bold-webfont.eot'); src: url('/fonts/vag_rounded_bold-webfont.eot?#iefix') format('embedded-opentype'),
    url('/fonts/vag_rounded_bold-webfont.woff') format('woff'), url('/fonts/vag_rounded_bold-webfont.ttf') format('truetype'), url('/fonts/vag_rounded_bold-webfont.svg#vag_rounded_bold-webfont') format('svg');
        font-weight: normal;  font-style: normal;
    }

    @font-face {
        font-family: 'vag_rounded_light';
        src: url('/fonts/vag_rounded_light-webfont.eot'); src: url('/fonts/vag_rounded_light-webfont.eot?#iefix') format('embedded-opentype'),
    url('/fonts/vag_rounded_light-webfont.woff') format('woff'), url('/fonts/vag_rounded_light-webfont.ttf') format('truetype'), url('/fonts/vag_rounded_light-webfont.svg#vag_rounded_light-webfont') format('svg');
        font-weight: normal;  font-style: normal;
    }

    @font-face {
        font-family: 'vag_rounded_black';
        src: url('/fonts/vag_rounded_black-webfont.eot'); src: url('/fonts/vag_rounded_black-webfont.eot?#iefix') format('embedded-opentype'),
    url('/fonts/vag_rounded_black-webfont.woff') format('woff'), url('/fonts/vag_rounded_black-webfont.ttf') format('truetype'), url('/fonts/vag_rounded_black-webfont.svg#vag_rounded_black-webfont') format('svg');
        font-weight: normal;  font-style: normal;
    }

    /* Bind to a class */
    .font-vag_rounded_bold { font-family: "vag_rounded_bold", Arial, Helvetica, sans-serif; }
    .font-vag_rounded_light { font-family: "vag_rounded_light", Arial, Helvetica, sans-serif; }
    .font-vag_rounded_black { font-family: "vag_rounded_black", Arial, Helvetica, sans-serif; }

    /*- Custom Sets -*/
    @font-face {
        font-family: 'font1';
        src: url('/fonts/vag_rounded_light.eot'); src: url('/fonts/vag_rounded_light.eot?#iefix') format('embedded-opentype'),
    url('/fonts/vag_rounded_light.woff') format('woff'), url('/fonts/vag_rounded_light.ttf') format('truetype'), url('/fonts/vag_rounded_light.svg#open_sanslight') format('svg');
        font-weight: normal;  font-style: normal;
    }

    @font-face {
        font-family: 'font2';
        src: url('/fonts/vag_rounded_bold.eot'); src: url('/fonts/vag_rounded_bold.eot?#iefix') format('embedded-opentype'),
    url('/fonts/vag_rounded_bold.woff') format('woff'), url('/fonts/vag_rounded_bold.ttf') format('truetype'), url('/fonts/vag_rounded_bold.svg#open_sansregular') format('svg');
        font-weight: normal; font-style: normal;
    }

    @font-face {
        font-family: 'font3';
        src: url('/fonts/vag_rounded_black.eot'); src: url('/fonts/vag_rounded_black.eot?#iefix') format('embedded-opentype'),
    url('/fonts/vag_rounded_black.woff') format('woff'), url('/fonts/vag_rounded_black.ttf') format('truetype'), url('/fonts/vag_rounded_black.svg#open_sanssemibold') format('svg');
        font-weight: normal; font-style: normal;
    }



    /* Bind to a class */
    .font1 { font-family: "font1", Arial, Helvetica, sans-serif; }
    .font2 { font-family: "font2", Arial, Helvetica, sans-serif; }
    .font3 { font-family: "font3", Arial, Helvetica, sans-serif; }

    /*******************************
           BOILERPLATE STYLES
    *******************************/
    /*HTML5*/
    article, aside, details, figcaption, figure, footer, header, hgroup, nav, section { display: block; }
    audio, canvas, video { display: inline-block; *display: inline; *zoom: 1; }
    audio:not([controls]) { display: none; }

    body {
        background-color: #FFF;
        text-align: center;
        line-height: 1;
        font-size: 100%;
        min-height:100%;
        vertical-align: baseline;

    }

    html, body {  width:100%; height:100%;  padding:0; margin:0; }

    a:visited { color: inherit; }
    a:hover, a:active { outline: 0; }
    .hidden { display: none !important; visibility: hidden; }
    .centerText { text-align: center !important; }


    /*******************************
           CONTAINERS
    *******************************/
    #divCONTAINER {
        width: 100%; margin: 0 auto;
        text-align: center;
        position: relative; z-index: 1000;
        clear: both; float: left;
        box-sizing: border-box;
    }

    .innerContainer {
        display: table;
        width: 1400px;
        margin: 0 auto;
    }

    /*******************************
        HEADER
    *******************************/
    #divHEADER {
        position: fixed;
        width: 100%; height: 100px;
        margin: 0 auto;
        box-sizing:  border-box;
        padding-top: 20px;
        z-index: 3000;
        background-color: #FFF;
    }

    #divHEADER .branding {
        display: table;
        float: left;
        clear: none;
        width: 30%;
        text-align: left;
        margin: 15px 0 0 0;
        cursor: pointer;
    }


    /******* Navigation *********/
    /**** BizKnect Orange #f54b00 *****/
    #divNAVIGATION {
        display: table;
        float: right;
        clear: none;
        width: 65%;
        text-align: right;
    }

    #divNAVIGATION .item {
        display: table-cell;
        float: right;
        clear: none;
        color: #7f7f7f;
        padding: 25px 20px 0; box-sizing: border-box;
        font-family: "vag_rounded_bold", Arial, Helvetica, sans-serif;
        font-size: 120%; letter-spacing: 2px;
        vertical-align: middle;
        cursor: pointer;
    }

    #divNAVIGATION .item:hover {
        color: #f54b00;
    }

    /**************************
        BODY CLASSES (and innerContainers)
    **************************/
    #divBODY {
        display: table;
        width: 100%;
        margin-top: 160px;
    }

    /******************************
            FOOTER STYLES
    *******************************/
    #divFOOTER {
        position: relative;
        width: 100%;
        height: auto;
        padding: 50px 30px 40px;
        box-sizing: border-box;
        line-height: 120%;
        display: table;
        vertical-align: bottom;
        bottom: 0;
        background-color: #7F7F7F;
    }

    #divFOOTER .links {
        font-family: "vag_rounded_light", Arial, Helvetica, sans-serif;
        font-size: 90%;
        margin: 0 auto;
        width: auto;
        box-sizing: border-box;
        color: #FFF;
    }

    #divFOOTER .links span { display: inline-block; cursor: pointer; padding: 0 20px;}
    #divFOOTER .links span:hover { text-decoration: underline; }

    #divFOOTER .dividerLine {
        height: 1px;
        background-color: #FFF;
        opacity: .3;
        width: 100%;
        margin: 30px auto;
    }

    #divFOOTER .disclaimer {
        font-family: "vag_rounded_light", Arial, Helvetica, sans-serif;
        font-size: 80%;
        line-height: 180%;
        display: table;
        margin: 20px auto 30px;
        width: 90%;
        opacity: .8;
        color: #FFF;
    }

    /**************************
       BTN STYLES
    **************************/
    .btn_obj {
        font-size: 120%;
        border-radius: 8px;
        margin: auto; padding: 18px 30px 18px;
        box-sizing: border-box;
        width: auto;
        display: table;
    }

    .btn_obj_inner {
        font-size: 90%;
        border-radius: 8px;
        margin: auto; padding: 12px 18px;
        box-sizing: border-box;
        width: auto;
        display: table;
    }

    .btn_obj_active {  cursor: pointer; opacity: 1; }
    .btn_obj_inactive {  cursor: default; opacity: .3;  }

    .btn_obj_color1 { background-color: #60b160; color: #FFF; }
    .btn_obj_color1:hover { background-color: #3b8d3b; color: #FFF; }

    .btn_obj_color2 { background-color: #608eb1; color: #FFF; }
    .btn_obj_color2:hover { background-color: #1377b8; color: #FFF;}

    .btn_obj_color3 { background-color: #b16060; color: #FFF; }
    .btn_obj_color3:hover { background-color: #b81313; color: #FFF; }

    .btn_obj_color4 { background-color: #888; color: #FFF;}
    .btn_obj_color4:hover { background-color: #464646; color: #FFF; }

</style>